<% if @instagram_pics %>
<div class="container">
  <h1><%= @pretendee.name %>'s Recent Instagram Pics</h1>
  <div class="row test">
    <div class="portfolio-box iso-call work-col-8">
      <% @instagram_pics.each do |pic|%>
      <div class="image-wrapper col-sm-3 nopadding">
        <img src="<%= pic.images.standard_resolution.url %>" alt="" class="img-responsive">
        <div class="image-overlay"> 
        </div>
      </div>
      <% end %>
      <% end %>
    </div>
  </div>
</div>